<div class="left">
	<h1>All Users</h1>
	<ul id="users-list">
		<% @users.each do |user| %>
			<li>
				<div class="user">
					<strong><%= link_to user.get_name, user_path(user) %></strong> <span class="grey"><em>(<%= user.username %>)</em></span>
							<% if current_user.admin? %>
									| <%= link_to 'Edit', edit_user_path(user) %>
							<% end %>
							<br />
					<span class="grey"><%= user.get_access_level_name %></span>
				</div>
			</li>
		<% end %>
	</ul>
</div>