<!DOCTYPE html>
<html>
	<head>
  	<title><%= title %></title>
  	<%= stylesheet_link_tag :all %>
  	<%= javascript_include_tag :defaults %>
  	<%= csrf_meta_tag %>
	</head>
	<body>
		<% flash.each do |key, value| %>
			<div class="flash-<%= key %>"><%= raw(value) %></div>
		<% end %>
		<%= yield %>
		<%= debug(params) if Rails.env.development? %>
	</body>
</html>
